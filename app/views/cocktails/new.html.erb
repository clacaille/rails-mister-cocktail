<div class="new">
  <div class="add-restaurant">
    <%= simple_form_for(@cocktail) do |f| %>

      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>

      <%= f.submit "Add your cocktail", class: "btn btn-warning text-center" %>
    <% end %>
  </div>
</div>
